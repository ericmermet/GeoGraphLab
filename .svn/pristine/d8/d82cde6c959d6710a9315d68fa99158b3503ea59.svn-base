package fr.ign.cogit.geographlab.test;

import fr.ign.cogit.geographlab.ihm.constantes.ConstantesApplication;

/** classe correspondant au mode debug */
public class Debug {
	/** affiche la chaine s si on est en mode debug */
	public static void printDebug(String s) {
		if (ConstantesApplication.debug == true)
			System.out.println(s);
	}
	
}
